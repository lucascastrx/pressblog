<%- include('./partials/header.ejs') %>
  <body>
    <%- include('./partials/homenavbar.ejs') %>
    <div class="container">
      <h1>Bem-vindo</h1>
      <% articles.forEach(article =>{ %>
        <div class="card">
          <div class="card-header">
            <h2><%= article.title %></h2>
          </div>
          <div class="card-body">
            <a href="<%= article.slug %>" class="btn btn-success" >Ler artigos</a>
          </div>
        </div>
        <br>
      <% }) %>
    </div>
   <%- include('./partials/footer.ejs') %>
   </body>
</html>
